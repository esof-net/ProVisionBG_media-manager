{
  "name": "provision/media-manager",
  "description": "ProVision Administration media manager",
  "keywords": [
    "provision",
    "module",
    "media manager"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Venelin Iliev",
      "homepage": "http://veneliniliev.com"
    }
  ],
  "support": {
    "issues": "https://github.com/ProVisionBG/media-manager/issues",
    "source": "https://github.com/ProVisionBG/media-manager"
  },
  "homepage": "http://provision.bg",
  "require": {
    "provision/administration": "^11.0"
  },
  "repositories": [
    {
      "url": "../administration",
      "type": "path"
    },
    {
      "url": "../translation",
      "type": "path"
    }    
  ],  
  "autoload": {
    "psr-4": {
      "ProVision\\MediaManager\\": "."
    }
  },
  "autoload-dev": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ProVision\\MediaManager\\": "."
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProVision\\MediaManager\\Providers\\ModuleServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
